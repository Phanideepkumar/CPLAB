#include<stdio.h>
#include<math.h>
int main () {
float C,F;
printf("\n enter temperature in C :");
scanf("%f",&C);
F=(9/5)*C+32 ;
printf("\ntemperature in F =%f", F);
printf("\n");
printf("\n enter temperature in F :");
scanf("%f",&F);
C = (F-32)/(9/5) ;
printf("\ntemperature in C =%f", C);
return 0;
}
