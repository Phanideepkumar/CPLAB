#include<stdio.h>
int main () {
int a,b,c,d;
printf("enter the values of a,b,c,d:");
scanf("%d%d%d%d",&a,&b,&c,&d);
if(a>b&&a>c&&a>d)
    printf("a is big");
else if(b>c&&b>a&&b>d)
     printf("b is big");
else if(c>a&&c>b&&c>d)     
        printf("c is big");
  else
      printf("d is big");
      } 
