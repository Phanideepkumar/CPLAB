#include<stdio.h>
#include<math.h>
int main() {
int a,b,c;
float d,r1,r2;
printf("\nenter the value of a,b,c; ");
scanf("%d%d%d",&a,&b,&c);

d=sqrt((b*b)-(4*a*c));
r1=(-b+d)/2*a ;
r2=(-b-d)/2*a ;
 
if(d>0)
       printf("roots are real  : %f ,%f",r1,r2);
else if(d==0)
              printf("roots are equal : %f ,%f",r1,r2);
else
    printf("roots are complex");
    }
  
