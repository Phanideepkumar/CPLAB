#include<stdio.h>
int main () {
int flag,key,n,i,j,position;
printf("\nenter size of array :");
scanf("%d",&n);
int a[n];
for(i=0;i<n;i++) {
    scanf("%d",&a[i]);}
 printf("enter a number to search : ");   
 scanf("%d",&key);
 flag=0;
 for(j=0;j<n;j++) {
     if(a[j]==key){
       flag++;
       position=j+1 ;}
       }
       if (flag>0) {
           printf("%d is found at %d position",key,position); }
       else
          printf("%d is not found ",key);
}        
          
