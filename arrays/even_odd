#include<stdio.h>
int main () { 
int even=0,odd=0,i;
 int a[5];
printf("enter the numbers ; ");
 for(i=0;i<5;i++) {
    scanf("%d",&a[i]); }
   
        for(i=0;i<5;i++) {
            if(a[i]%2==0)
                even++;
            else 
                odd++;
                }
 printf("no of even element = %d",even);
 printf("\nno of odd numbers = %d",odd);
  
  }                   
